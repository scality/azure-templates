{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmSku": {
      "value": "Standard_DS2_v2"
    },
    "vmssName": {
      "value": "<yourVMSSName>"
    },
    "instanceCount": {
      "value": 2
    },
    "adminUsername": {
      "value": "ubuntu"
    },
    "sshPublicKey": {
       "value": "ssh-rsa <your public key>"
    },
    "scalityConnectImageURI": {
      "value": "https://yourvhdimageuri.vhd"
    },
    "storageAccountName": {
      "value": "<yourStorageAccountName>"
    },
    "storageAccountAccessKey": {
      "value": "<youStorageAccountKey>"
    },
    "vaultResourceGroup": {
     "value": "REPLACE_VAULTRG"
    },
    "vaultName": {
      "value" : "REPLACE_VAULTNAME"
    },
    "caCertThumbPrint" :
    {
        "value": "REPLACE_CACERTPRINT"
    },
    "httpssecretCaUrlWithVersion": {
      "value": "REPLACE_CACERTURL"
    },
    "CACertSAS": {
      "value": "<SAS url to download your CA certificates>"
    },
    "domainName": {
      "value": "yourdomain.name"
    }
  }
}
